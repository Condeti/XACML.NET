<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="Xacml.net" type="Xacml.Core.Configuration.SectionHandler, Xacml.Core"></section>
	</configSections>
	<Xacml.net>
		<attributeRepositories>
			<repository name="default" type="Xacml.Core.Extensions.DefaultAttributeRepository, Xacml.Core">
				<Attribute AttributeId="urn:oasis:names:tc:xacml:1.0:subject:subject-id">
					<AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">Julius Hibbert</AttributeValue>
				</Attribute>
				<Attribute AttributeId="urn:oasis:names:tc:xacml:1.0:example:attribute:role">
					<AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">Physician</AttributeValue>
				</Attribute>
			</repository>
		</attributeRepositories>
		<policyRepositories>
			<repository name="default" type="Xacml.Core.Extensions.DefaultPolicyRepository, Xacml.Core">
				<PolicySet PolicySetId="urn:oasis:names:tc:xacml:1.0:conformance-test:IIE001:policyset1" FilePath="IIE001PolicySetId1.xml"/>
				<PolicySet PolicySetId="urn:oasis:names:tc:xacml:1.0:conformance-test:IIE002:policyset1" FilePath="IIE002PolicySetId1.xml"/>
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IIE001:policy1" FilePath="IIE001PolicyId1.xml"/>
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IIE002:policy1" FilePath="IIE002PolicyId1.xml"/>
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IIE003:policy1" FilePath="IIE003PolicyId1.xml"/>
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IIE003:policy2" FilePath="IIE003PolicyId2.xml"/>
				
				<!-- In order to succesfully execute the IID029 test. The following lines must be the ONLY policies in the repository
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IID029:policy1" FilePath="IID029Policy1.xml" />
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IID029:policy2" FilePath="IID029Policy2.xml" />
				-->

				<!-- In order to succesfully execute the IID030 test. The following lines must be the ONLY policies in the repository
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IID030:policy1" FilePath="IID030Policy1.xml" />
				<Policy PolicyId="urn:oasis:names:tc:xacml:1.0:conformance-test:IID030:policy2" FilePath="IID030Policy2.xml" />
				-->
			</repository>
		</policyRepositories>
		<functionRepositories>
			<repository name="default" type="Xacml.Core.Extensions.DefaultFunctionRepository, Xacml.Core">
			</repository>
		</functionRepositories>
		<dataTypeRepositories>
			<repository name="default" type="Xacml.Core.Extensions.DefaultDataTypeRepository, Xacml.Core"/>
		</dataTypeRepositories>
	</Xacml.net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/></startup></configuration>
